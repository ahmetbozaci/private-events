

<div class='content'>
<% if logged_in?  %>
  <%= current_user.created_events.count %>
    <%= link_to 'Create Event', events_new_path, class: 'button is-light' %>
  <% current_user.created_events.each do |created_event|%>
    <h3 class='subtitle'><%= created_event.title %></h3>
    <p> <%= created_event.description %> <span><%= created_event.date %></span></p>
  <% end %>

<% else %>
  <h1>No events </h1>
   <%= link_to 'Create Event', events_new_path, class: 'button is-light' %>
<% end %>

</div>

